{
  "name": "LinkedIn Pronoun Displayer",
  "version": "1.1",
  "description": "Displays a person's pronouns under their name on LinkedIn on Chrome (if provided).",
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["linked_in_pronoun_displayer_script.js"]
    }
  ],
  "icons": {
    "16": "public/images/icon16.png",
    "48": "public/images/icon48.png",
    "128": "public/images/icon128.png"
  },
  "manifest_version": 2
}
